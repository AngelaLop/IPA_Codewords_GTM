preserve 

drop if empowerment_1==. & tment==2
iebaltab edad_nacimiento primary_w primary_head home_2  single ln_land aplica_mam head_self farm_4 digitagro_baseline, grpvar(tment) savetex("$tables/balance_cluster_effective.tex") vce(cluster village_id) pt ft pf fmissok grplabels("0 Control @ 1 Code words @ 2 Keyboard") rowlabels("edad_nacimiento Interviewee Age @ primary_w Interviewee finished primary education @ primary_head Household head finished primary education @ home_2 Number of household members @ single Interviewee is single  @ ln_land Log Land Size (in cuerdas) @ aplica_mam Interviewee asked interview in mam @ head_self Interviewee is HH head @ farm_4 Land Ownership @ digitagro_baseline Interviewee was in Baseline" ) replace
restore 




*===============================================================================

					*Placebo Table 

*===============================================================================



*Variable Construction


destring placebo_t1 placebo_1t placebo_k,replace
replace placebo_t1 =0 if placebo_t1==2
replace placebo_1t=0 if placebo_1t==2
replace placebo_k =0 if placebo_k==2
label values placebo_t1 answer1
label values placebo_1t answer1
label values placebo_k answer1
egen placebo_c = rowmax(placebo_1c placebo_c1)
egen placebo_n = rowmax(placebo_1n placebo_n1)
egen placebo_begin = rowmax(placebo_c1 placebo_n1 placebo_t1)
egen placebo_end = rowmax(placebo_1c placebo_1n placebo_1t)
egen placebo_all = rowmax(placebo_c placebo_n placebo_k)


*===============================================================================
					
					*Table for Predicting Missing Values
					
*===============================================================================

***Construction of Missing Values for SE Answers
gen miss1=1 if missing(empowerment_1)
replace miss1=0 if miss1==.
label var miss1 "Q1 missing"

gen miss2=1 if missing(empowerment_2)
replace miss2=0 if miss2==.
label var miss2 "Q2 missing"

gen miss3=1 if missing(empowerment_3)
replace miss3=0 if miss3==.
label var miss3 "Q3 missing"

gen miss4=1 if missing(empowerment_4)
replace miss4=0 if miss4==.
label var miss4 "Q4 missing"

gen miss5=1 if missing(empowerment_5)
replace miss5=0 if miss5==.
label var miss5 "Q5 missing"

*Note: Q6 was only asked for women who have children 

gen miss6=1 if missing(empowerment_6) & home_3>0
replace miss6=0 if miss6==. & home_3>0
label var miss6 "Q6 missing"


*Table for Prediction of Missing Values 


global y_vars miss1 miss2 miss3 miss4 miss5 miss6
local i=1
foreach var of global y_vars{
	qui reg `var' edad_nacimiento aplica_mam primary_w primary_head ln_land home_2 single head_self digitagro_baseline farm_4, vce(cluster village_id)
	est sto m`i'
	qui testparm*
	estadd scalar p_value = r(p)
	local ++i
}	
*esttab m1 m2 m3 m4 m5 m6 using "$tables/missings.tex", se ar2 label scalar(F p_value) title(Predicting missing values with covariates) star("*" 0.10 "**" 0.05 "***" 0.01) b(3) se(3) par replace 	


*===============================================================================
*Summary Statistics
*===============================================================================

*Sum Stats for Covariates 
*asdoc sum edad_nacimiento farm_2 i.farm_4 i.home_12 i.home_16 i.home_11 i.home_3 i.home_4 i.home_5 i.home_6 i.home_17 i.aplica_mam home_2, replace abb(.)

*Sum Stats for Outcomes
*asdoc sum i.miss1 i.miss2 i.miss3 i.miss4 i.miss5 i.miss6 i.empowerment_1 i.empowerment_2 i.empowerment_3 i.empowerment_4 i.empowerment_5 i.empowerment_6 i.placebo_begin i.placebo_end i.placebo_all, replace abb(.)


*===============================================================================

				* Indexes

*===============================================================================

* Variables for Vulnerability Index 
* (Note: Keep variables all in the same orientation)
*Variables: Dependency Ratio (Working Age Members/Non-Working Age Members)
*			Did not finish Primary
*			Does not Own Land
*			Interviewee is not HH Head 

gen dep_ratio = (home_3 + home_6)/(home_4 + home_5)
gen no_primary_w =1 if home_12<3
replace no_primary_w=0 if home_12 >2


*Local Macros (index variables, outcome variables, covariates)

local vul_index index dep_ratio head_self no_primary_w farm_4 single 
local empower index empowerment_1 empowerment_2 empowerment_3 empowerment_4 empowerment_5 empowerment_6
local covariates index edad_nacimiento aplica_mam single ln_land primary_w primary_head head_self head_partner home_2 home_3 home_4 home_5 digitagro_baseline farm_4

*Standardization of Variables for vulnerability index
foreach v in $`vul_index'{
	sum `v' 
	gen `v'_z = (`v' - r(mean))/r(sd)
}

*Creation of Vulnerability Index 

gen vulnerability = (dep_ratio_z +head_self_z+ no_primary_w_z+ farm_4_z)/4


*Standardization for outcome variables
foreach v in $`empower'{
	sum `v' 
	gen `v'_z = (`v' - r(mean))/r(sd)
}

*Assign Mean to Q6 for each group
replace empowerment_6mean = .5765472 if tment==0 & missing(empowerment_6mean)
replace empowerment_6mean = .4984985 if tment==1 & missing(empowerment_6mean) 
replace empowerment_6mean = .5 if tment==2 & missing(empowerment_6mean) 

*Index Construction

gen avg = (empowerment_1 + empowerment_2 + empowerment_3 + empowerment_4 + empowerment_5 + empowerment_6)/6
label var avg "Average Index"

gen standard = (empowerment_1_z + empowerment_2_z + empowerment_3_z +empowerment_4_z+empowerment_5_z+empowerment_6_z)/6
label var standard "Standardized Index"


gen index_Q1Q5 = (empowerment_1 + empowerment_2 + empowerment_3 + empowerment_4 + empowerment_5)/5
label var index_Q1Q5 "Index from Q1-Q5"


gen empowerment_0 = (empowerment_1 + empowerment_2 + empowerment_3 + empowerment_4 + empowerment_5 + empowerment_6mean)/6
label var empowerment_0 "Index"

est clear 

*for fixed effects
egen muninum=group(municipality)
egen villagen=group(village_id)
*pooled treatment 
gen privacy = 1 if tment!=0
replace privacy =0 if privacy==.
label var privacy "Added Privacy"


*Table for Placebo Question

iebaltab placebo_all placebo_begin placebo_end, grpvar(tment) savetex("$tables/placebo.tex") vce(cluster village_id) fmissok grplabels("0 Control @ 1 Code words @ 2 Keyboard") pt rowlabels("placebo_all Is this month harvest season? (All interviewees) @ placebo_begin Is this month harvest season? (Question was asked at the beginning of the interview) @ placebo_end Is this month harvest season? (Question was asked at the end of the interview) ") replace

* Tables for placebo - vulnerability 
iebaltab placebo_all placebo_begin placebo_end if vulnerability <= -.0505324, grpvar(tment) savetex("$tables/placebo_vulnera_low.tex") vce(cluster village_id) fmissok grplabels("0 Control @ 1 Code words @ 2 Keyboard") pt rowlabels("placebo_all Is this month harvest season? (All interviewees)") replace

iebaltab placebo_all placebo_begin placebo_end if vulnerability > -.0505324, grpvar(tment) savetex("$tables/placebo_vulnera_high.tex") vce(cluster village_id) fmissok grplabels("0 Control @ 1 Code words @ 2 Keyboard") pt rowlabels("placebo_all Is this month harvest season? (All interviewees)") replace

